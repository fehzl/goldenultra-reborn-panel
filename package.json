{
  "name": "goldenultra",
  "version": "1.0.0",
  "private": true,
  "author": "Felipe Feliciano <fehzl@live.com> (@fehzl)",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz",
    "test": "jest --passWithNoTests --no-cache --runInBand"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "2.8.8",
    "axios": "0.26.0",
    "next": "12.0.10",
    "next-auth": "4.2.1",
    "puppeteer": "13.4.1",
    "react": "17.0.2",
    "react-currency-input-field": "3.6.4",
    "react-dom": "17.0.2",
    "react-hook-form": "7.27.0",
    "react-icons": "4.3.1",
    "react-qr-code": "2.0.3",
    "react-query": "3.34.15",
    "react-select": "5.2.2",
    "tailwind-scrollbar-hide": "1.1.7",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@testing-library/jest-dom": "5.16.2",
    "@types/node": "16.11.24",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "autoprefixer": "10.4.2",
    "commitizen": "4.2.4",
    "commitlint-config-gitmoji": "2.2.5",
    "cypress": "9.5.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.9.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.3",
    "postcss": "8.4.6",
    "prettier": "2.5.1",
    "tailwindcss": "3.0.23",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
